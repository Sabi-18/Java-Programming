import java.util.*;
public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt(),sum=0;
        if(a<=99){
            System.out.println("Invalid Input");
        }
        else{
            while(a>0){
            sum += a % 10;
            a = (a / 10);
            }
        System.out.println("Sum of digit is "+sum);
    }
       
    }
}
